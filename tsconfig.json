{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "@fengkx/tsconfig",
	"compilerOptions": {
		"target": "es2022",
		"module": "esnext",
		"sourceMap": true,
		"outDir": "out",
		"noEmit": true,
		"rootDir": ".",
		"strict": true,
		"esModuleInterop": true,
		"composite": true,
		"skipLibCheck": true,
		"isolatedModules": true,
		"verbatimModuleSyntax": false
	}
}
