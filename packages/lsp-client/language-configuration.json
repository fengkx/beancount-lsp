{
	"comments": {
		"lineComment": ";"
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"]
	],
	"autoClosingPairs": [
		{ "open": "{", "close": "}" },
		{ "open": "[", "close": "]" },
		{ "open": "(", "close": ")" }
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""]
	],
	//   "folding": {
	//     "markers": {
	//       "start": "^[0-9]{4}[-/][0-9]{2}[-/][0-9]{2}\\s+\\*",
	//       "end": "^(?=[0-9]{4}[-/][0-9]{2}[-/][0-9]{2})"
	//     }
	//   },
	"wordPattern": "(-?\\d+\\.\\d+)|([^\\s\\;\\\"\\(\\)\\,\\{\\}\\[\\]\\<\\>\\'\\`\\:\\\\]+)"
}
